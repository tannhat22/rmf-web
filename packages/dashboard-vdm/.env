REACT_APP_VERSION = v2.1.0
GENERATE_SOURCEMAP = false

## Backend API URL
# REACT_APP_API_URL=http://220.135.101.116
# REACT_APP_API_PORT=8000

## Auth0

# REACT_APP_AUTH0_CLIENT_ID=
# REACT_APP_AUTH0_DOMAIN=

# because we are a monorepo, some create-react-app dependencies may be installed in the root directory,
# it complains if it find such packages so we need to disable the check.
SKIP_PREFLIGHT_CHECK=true

REACT_APP_TRAJECTORY_SERVER=ws://localhost:8006
REACT_APP_RMF_SERVER=http://localhost:8000
# REACT_APP_TRAJECTORY_SERVER=ws://192.168.47.128:8006
# REACT_APP_RMF_SERVER=http://192.168.47.128:8000

REACT_APP_AUTH_PROVIDER=keycloak
REACT_APP_KEYCLOAK_CONFIG={"url":"http://localhost:8080/auth","realm":"rmf-web","clientId":"rmf_api_server"}
# REACT_APP_KEYCLOAK_CONFIG={"url":"http://192.168.47.128:8080/auth","realm":"rmf-web","clientId":"rmf_api_server"}